{
  "name": "candilib",
  "version": "0.1.0",
  "description": "Candilib Isomorphic apps using React and Redux",
  "scripts": {
    "dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "test": "cross-env NODE_ENV=test MONGO_URL=mongodb://localhost:27017/candilib node_modules/.bin/nyc node node_modules/ava/cli.js --no-color",
    "test:watch": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development node index.js",
    "start:prod": "cross-env NODE_ENV=production pm2 --no-daemon index.js",
    "bs": "npm run clean && npm run build && npm run build:server && npm run start:prod",
    "prebuild": "run-p lint test",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "clean": "rimraf dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint ./",
    "import:json": "mongoimport --db candilib --collection candidats --type json --file candidatsLibresEnrichis_20180822_110005.json --jsonArray",
    "import:csv": "mongoimport --db candilib --collection candidats --type csv --file candilib_iso.csv --headerline"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hashnode/mern-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Hashnode/mern-starter/issues"
  },
  "homepage": "https://github.com/Hashnode/mern-starter#readme",
  "author": "Prashant Abhishek <prashant.abhishek7g@gmail.com>, Mayank Chandola <imayankchd@gmail.com>, Sandeep Panda <sandeep@hashnode.com>, Syed Fazle Rahman <fazle@hashnode.com>, Alkshendra Maurya <alkshendra@hashnode.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.15.1",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "csv-express": "^1.2.2",
    "csvtojson": "^2.0.8",
    "cuid": "^2.1.4",
    "dotenv": "^6.2.0",
    "downshift": "^3.1.5",
    "express": "^4.16.4",
    "express-fileupload": "^1.0.0",
    "fast-csv": "^2.4.1",
    "iconv-lite": "^0.4.24",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^4.7.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "npm-run-all": "^4.1.5",
    "sanitize-html": "^1.11.4",
    "uglifyjs-webpack-plugin": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.2.2",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "cssnano": "^3.7.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^2.0.0",
    "jsdom": "^13.0.0",
    "nock": "^10.0.3",
    "nodemon": "^1.18.7",
    "null-loader": "^0.1.1",
    "nyc": "^13.1.0",
    "pre-commit": "^1.1.3",
    "raf": "^3.4.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.19.1",
    "supertest": "^3.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^3.11.0",
    "webpack2-externals-plugin": "^1.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "ava": {
    "files": [
      "config.spec.js",
      "./**/*.spec.js"
    ],
    "sources": [
      "./**/*.js"
    ],
    "failFast": true,
    "babel": {
      "testOptions": {
        "babelrc": false,
        "configFile": false,
        "plugins": [
          "@babel/plugin-syntax-dynamic-import",
          [
            "@babel/plugin-proposal-decorators",
            {
              "legacy": true
            }
          ],
          [
            "@babel/plugin-proposal-class-properties",
            {
              "loose": false
            }
          ]
        ],
        "presets": [
          [
            "module:ava/stage-4",
            false
          ]
        ]
      }
    },
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "nyc": {
    "include": [
      "./**/*.js"
    ],
    "exclude": [
      "./**/*.spec.js",
      "./util/setup-test-env.js",
      "./config.js",
      "./dummyData.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  }
}
