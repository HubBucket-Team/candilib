{
  "name": "candilib",
  "version": "0.1.0",
  "description": "Candilib Isomorphic apps using React and Redux",
  "scripts": {
    "test": "cross-env NODE_ENV=test MONGO_URL=mongodb://localhost:27017/candilib node_modules/.bin/nyc node node_modules/ava/cli.js --no-color",
    "test:watch": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development node index.js",
    "start:prod": "cross-env NODE_ENV=production WEB_PORT=80 pm2 --no-daemon index.js",
    "bs": "npm run clean && npm run build && npm run build:server && npm run start:prod",
    "prebuild": "run-p lint test",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "clean": "rimraf dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint server",
    "import:json": "mongoimport --db candilib --collection candidats --type json --file candidatsLibresEnrichis_20180822_110005.json --jsonArray",
    "import:csv": "mongoimport --db candilib --collection candidats --type csv --file candilib_iso.csv --headerline"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hashnode/mern-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Hashnode/mern-starter/issues"
  },
  "homepage": "https://github.com/Hashnode/mern-starter#readme",
  "author": "Prashant Abhishek <prashant.abhishek7g@gmail.com>, Mayank Chandola <imayankchd@gmail.com>, Sandeep Panda <sandeep@hashnode.com>, Syed Fazle Rahman <fazle@hashnode.com>, Alkshendra Maurya <alkshendra@hashnode.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@devexpress/dx-react-core": "^1.9.1",
    "@devexpress/dx-react-grid": "^1.9.1",
    "@devexpress/dx-react-grid-material-ui": "^1.9.1",
