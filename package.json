    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack2-externals-plugin": "^1.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "ava": {
    "files": [
      "server/**/*.spec.js"
    ],
    "sources": [
      "server/**/*.js"
    ],
    "failFast": true,
    "babel": {
      "testOptions": {
        "babelrc": false,
        "configFile": false,
        "plugins": [
          "@babel/plugin-syntax-jsx",
          "@babel/plugin-syntax-dynamic-import",
          [
            "@babel/plugin-proposal-decorators",
            {
              "legacy": true
            }
          ],
          [
            "@babel/plugin-proposal-class-properties",
            {
              "loose": false
            }
          ]
        ],
        "presets": [
          "@babel/preset-react",
          [
            "module:ava/stage-4",
            false
          ]
        ]
      }
    },
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "nyc": {
    "include": [
      "client/**/*.js",
      "server/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "client/reducers.js",
      "client/store.js",
      "client/routes.js",
      "server/util/setup-test-env.js",
      "server/config.js",
      "server/dummyData.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  }
}
