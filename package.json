    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "mongoose": "^4.13.17",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^4.7.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-big-calendar": "^0.20.2",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.2.1",
    "react-router-dom": "^4.3.1",
